{
  "name": "demo_source_connector",
  "connector.class": "io.debezium.connector.mysql.MySqlConnector",
  "key.converter": "org.apache.kafka.connect.json.JsonConverter",
  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "transforms": "unwrap, AddNamespace, ExtractField",
  "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
  "transforms.unwrap.delete.handling.mode": "rewrite",
  "transforms.unwrap.drop.tombstones": "false",
  "transforms.AddNamespace.type": "org.apache.kafka.connect.transforms.SetSchemaMetadata$Value",
  "transforms.AddNamespace.schema.name": "com.example.model.Customer",
  "transforms.ExtractField.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
  "transforms.ExtractField.field": "customerNumber",
  "database.hostname": "localhost",
  "database.port": "3306",
  "database.user": "root",
  "database.password": "<notarealpassword>",
  "database.server.name": "avroconnect",
  "database.history.kafka.bootstrap.servers": "localhost:9092",
  "database.history.kafka.topic": "debizium-mysql-history",
  "include.schema.changes": "false",
  "table.ignore.builtin": "true",
  "database.whitelist": "poc",
  "key.converter.schemas.enable": "false",
  "value.converter.schema.registry.url": "http://localhost:8081",
  "internal.value.converter.schemas.enable": "false",
  "value.converter.schemas.enable": "true",
  "internal.value.converter": "org.apache.kafka.connect.json.JsonConverter"
}